    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "sns.set(font='Segoe UI')\n",
    "\n",
    "from scipy.stats import poisson\n",
    "import random\n",
    "import scipy.stats as stats"
